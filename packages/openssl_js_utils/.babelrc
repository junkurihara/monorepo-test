{
  "presets": [
    [ "@babel/preset-env", {
      "targets": { },
      "useBuiltIns": false
    } ],
    "babel-preset-minify"
  ],
  "ignore": [ "node_modules" ],
  "only": [ "src", "test" ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-runtime",
      {
        "@babel/polyfill": false,
        "regenerator": true
      }
    ]
  ]
}